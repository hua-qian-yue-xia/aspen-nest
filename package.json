{
	"name": "aspen-nest",
	"version": "1.0.0",
	"description": "nest多模块快速开发系统",
	"private": true,
	"scripts": {
		"admin:dev": "NODE_ENV=dev nest start @aspen/admin --debug --watch",
		"admin:build": "NODE_ENV=prod nest build @aspen/admin",
		"app:dev": "NODE_ENV=dev nest start @aspen/app --debug --watch",
		"app:build": "NODE_ENV=prod nest build @aspen/app",
		"format": "prettier --write \"**/*.{ts,js,json,md}\"",
		"lint": "eslint \"{apps,libs}/**/*.ts\" --fix",
		"prepare": "husky"
	},
	"author": "",
	"license": "MIT",
	"workspaces": {
		"packages": [
			"./apps/*",
			"./packages/*"
		]
	},
	"dependencies": {
		"@fastify/static": "^7.0.0",
		"@liaoliaots/nestjs-redis": "^10.0.0",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "^10.0.0",
		"@nestjs/jwt": "^11.0.0",
		"@nestjs/mapped-types": "^2.1.0",
		"@nestjs/passport": "^11.0.5",
		"@nestjs/platform-fastify": "10.4.20",
		"@nestjs/swagger": "11.2.0",
		"@nestjs/typeorm": "^11.0.0",
		"@temporalio/activity": "^1.11.7",
		"@temporalio/client": "^1.11.7",
		"@temporalio/worker": "^1.11.7",
		"@temporalio/workflow": "^1.11.7",
		"bcrypt": "^6.0.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"dayjs": "^1.11.18",
		"fastify": "^5.2.1",
		"glob": "^11.0.3",
		"ioredis": "^5.4.2",
		"js-yaml": "^4.1.0",
		"ms": "^2.1.3",
		"mysql2": "^3.14.0",
		"nestjs-cls": "^5.4.1",
		"passport": "^0.7.0",
		"passport-jwt": "^4.0.1",
		"pinyin": "^4.0.0",
		"radash": "^12.1.0",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1",
		"typeorm": "^0.3.21",
		"typeorm-naming-strategies": "^4.1.0",
		"winston": "^3.18.3",
		"winston-daily-rotate-file": "^5.0.0",
		"winston-loki": "^6.1.3"
	},
	"devDependencies": {
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@types/bcrypt": "^6.0.0",
		"@types/js-yaml": "^4.0.9",
		"@types/ms": "^2.1.0",
		"@types/node": "20.3.1",
		"@types/passport-jwt": "^4.0.1",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"@typescript-eslint/parser": "^8.18.1",
		"eslint": "8.48.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.3",
		"prettier": "^3.4.2",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2"
	},
	"lint-staged": {
		"*.{ts,js}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md}": [
			"prettier --write"
		]
	}
}
